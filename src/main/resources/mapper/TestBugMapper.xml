<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.master.tsim.mapper.TestBugMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.master.tsim.entity.TestBug">
        <id column="id" property="id" />
        <result column="project_id" property="projectId" />
        <result column="module_id" property="moduleId" />
        <result column="title" property="title" />
        <result column="severity" property="severity" />
        <result column="function" property="function" />
        <result column="bug_status" property="bugStatus" />
        <result column="note" property="note" />
        <result column="tester" property="tester" />
        <result column="plan_id" property="planId" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_id, module_id, title, severity, function, bug_status, note, tester, plan_id, create_date, update_date
    </sql>

</mapper>
